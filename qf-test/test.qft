  <TestSet id="_1" name="unnamed">
    <SetupSequence id="_6" name="Launch jtimesched">
      <SetGlobalStep id="_7" varname="client">
        <default>jtimesched</default>
      </SetGlobalStep>
      <ClientWaiter client="$(client)" id="_8" local="true" raise="false"
                    resvarname="isSUTRunning" timeout="1">
        <comment>This node checks whether the SUT is already running. The result of this check will be stored in the variable isSUTRunning. The variable itself can contain true if SUT is already running or false if SUT is not running. This variable will be used in the following 'If' node.</comment>
      </ClientWaiter>
      <IfSequence id="_9" name="Launch SUT if not running"
                  test="not $(isSUTRunning)">
        <JavaClientStarter client="$(client)"
                           directory="/home/maruba/Documents/jtimesched"
                           executable="${qftest:java}" id="_A">
          <parameter>-jar</parameter>
          <parameter>/home/maruba/Documents/jtimesched/target/jtimesched-1.2-SNAPSHOT-jar-with-dependencies.jar</parameter>
        </JavaClientStarter>
        <ClientWaiter client="$(client)" id="_B"/>
      </IfSequence>
    </SetupSequence>
    <TestCase id="_2" name="create-project">
      <BasicSequence id="_Q" name="seq1">
        <comment>no popup opened -&gt;  new idle project -&gt; edit title -&gt; no popup opened </comment>
        <MouseEventStep clicks="1" client="$(client)"
                        component="buttonAdd_project" event="MOUSE_MPRC"
                        id="_R" modifiers="16"/>
        <KeyEventStep client="$(client)" component="frameJTimeSched.text"
                      event="KEY_PTR" id="_S" keychar="8" keycode="8"
                      modifiers="0"/>
        <TextInputStep clear="true" client="$(client)"
                       component="frameJTimeSched.text" id="_T">
          <text>Projeto1</text>
        </TextInputStep>
        <KeyEventStep client="$(client)" component="frameJTimeSched.text"
                      event="KEY_PTR" id="_U" keychar="10" keycode="10"
                      modifiers="0"/>
      </BasicSequence>
      <BasicSequence id="_u" name="Check project name">
        <CheckStringStep checktype="item" client="$(client)"
                         component="frameJTimeSched.table1_project_|_0:00:00_overall_|_0:00:00_today@Title&amp;0"
                         id="_v">
          <text>Projeto1</text>
        </CheckStringStep>
      </BasicSequence>
      <BasicSequence id="_11" name="seq2">
        <comment>Test the creation of a project with deault name.
2. No popup opened -&gt; new idle project -&gt; no popup opened</comment>
        <MouseEventStep clicks="1" client="$(client)"
                        component="buttonAdd_project" event="MOUSE_MPRC"
                        id="_12" modifiers="16"/>
        <KeyEventStep client="$(client)" component="frameJTimeSched.text"
                      event="KEY_PTR" id="_13" keychar="10" keycode="10"
                      modifiers="0"/>
      </BasicSequence>
      <BasicSequence id="_15" name="Check project name">
        <comment>The project name must be "New Project"</comment>
        <CheckStringStep checktype="item" client="$(client)"
                         component="frameJTimeSched.table2_projects_|_0:00:00_overall_|_0:00:00_today@Title&amp;1"
                         id="_16">
          <text>New project</text>
        </CheckStringStep>
      </BasicSequence>
      <CleanupSequence id="_w" name="clean">
        <MouseEventStep clicks="2" client="$(client)"
                        component="frameJTimeSched.table1_project_|_0:00:00_overall_|_0:00:00_today&amp;0&amp;0"
                        event="MOUSE_DBLCLK" id="_x" modifiers="16"/>
      </CleanupSequence>
    </TestCase>
